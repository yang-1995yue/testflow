    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "fast-check": "^4.3.0",
    "happy-dom": "^20.0.11",
    "postcss": "^8.5.6",
